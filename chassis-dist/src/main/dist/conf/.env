#==============================================================================
# Global Java Environment Variables
#==============================================================================

MY_JAVA_HOME=
JAVA_OPTS=
PID_DIR=/tmp
MAX_LOG_FILE_NUMBER=10

#==============================================================================
# Application
#==============================================================================

APP_DEV_MODE=true
APP_INSTANCE_GROUP=development
APP_TAGS=
SPRING_CONFIG_IMPORT=optional:file:conf/global/,optional:file:conf/global/*/,optional:file:conf/${APP_NAME}/

#==============================================================================
# logging
#==============================================================================

LOG_CONFIG=classpath:logback-file.xml
LOG_LEVEL_ROOT=INFO

#==============================================================================
# Consul Configuration (Experimental Feature)
#==============================================================================

CONSUL_ENABLED=false
CONSUL_HOST=127.0.0.1
CONSUL_PORT=8500

CONSUL_DISCOVERY_ENABLED=true
CONSUL_DISCOVERY_REGISTER_ENABLED=true
CONSUL_DISCOVERY_PREFER_IP=true

CONSUL_CONFIG_ENABLED=true
# spring.cloud.consul.watch.enbled=true目前有问题
CONSUL_CONFIG_WATCH_ENABLED=false
CONSUL_CONFIG_PREFIX=config

CONSUL_RETRY_ENABLED=true

